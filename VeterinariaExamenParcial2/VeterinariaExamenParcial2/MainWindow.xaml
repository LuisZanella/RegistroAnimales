        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VeterinariaExamenParcial2"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="300">
    <Grid>
        <Canvas>
            <Button x:Name="btnInicio" Content="Iniciar" Height="21" Canvas.Left="151" Canvas.Top="121" Width="88" Click="btnInicio_Click_1"/>
            <Label Content="Inicio de Sesión" Height="27" Canvas.Left="62" Canvas.Top="10" Width="177" HorizontalContentAlignment="Center" FontSize="16"/>
            <Label Content="Nombre Usuario" Height="23" Canvas.Left="23" Canvas.Top="49" Width="124"/>
            <Label Content="Contraseña" Height="27" Canvas.Left="23" Canvas.Top="82" Width="124"/>
            <TextBox x:Name="txtUserName" Height="27" Canvas.Left="127" TextWrapping="Wrap" Canvas.Top="51" Width="146"/>
            <PasswordBox x:Name="pwtPassword" Height="27" Canvas.Left="127" Canvas.Top="82" Width="146"/>

        </Canvas>
    </Grid>
</Window>
